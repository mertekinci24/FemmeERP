<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <!-- Direct dependencies -->
      <package pattern="CsvHelper" />
      <package pattern="QuestPDF" />
      <package pattern="ClosedXML" />
      <package pattern="AutoMapper*" />
      <package pattern="FluentValidation*" />
  <package pattern="MediatR*" />
  <package pattern="Xunit.*" />
  <package pattern="Xunit*" />
  <package pattern="xunit*" />
      <package pattern="SkiaSharp*" />
      <package pattern="SixLabors.*" />
      <!-- System packages (for CsvHelper and other dependencies) -->
      <package pattern="System.*" />
      <!-- Microsoft packages (transitive dependencies) -->
      <package pattern="Microsoft.*" />
      <!-- Common transitive dependencies -->
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>